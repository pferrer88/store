<li id="link-to-cart" data-hook><%= link_to_cart %></li>
<% if current_user %>
  <li><%= link_to t(:my_account), spree.account_path %></li>
  <li><%= link_to t(:logout), spree.destroy_user_session_path %></li>
<% else %>
  <li id="link-to-login"><%= link_to t(:login), spree.login_path %></li>
<% end %>
